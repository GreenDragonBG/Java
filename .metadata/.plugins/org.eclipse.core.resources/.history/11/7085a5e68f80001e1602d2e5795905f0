package Main;

public class Bag extends Item {
	private Item[] slots;

	public Bag(String name) {
		super(name);
		slots = new Item[6];
	}

	public void add(Item e) throws Exception {
		for (int i = 0; i < slots.length; i++) {
			if (slots[i] == null) {
				slots[i] = e;
				break;
			} else if (i == slots.length - 1) {
				throw new Exception("Exception message");
			}
		}
	}

}
